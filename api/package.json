{
  "name": "listspace-api",
  "version": "1.0.0",
  "description": "ListSpace API",
  "main": "dist/handlers/properties/handler.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "package": "npm run build && npm run package-lambda",
    "package-lambda": "cd dist && powershell \"Compress-Archive -Path '*' -DestinationPath '../api.zip' -Force\" && cd .. && powershell \"Compress-Archive -Update -Path 'node_modules/sharp/**', 'node_modules/@img/**', 'package.json' -DestinationPath 'api.zip'\"",
    "dev": "ts-node-dev --respawn --transpile-only local-server.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@aws-sdk/protocol-http": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@aws-sdk/url-parser": "^3.370.0",
    "@aws-sdk/util-format-url": "^3.969.0",
    "@types/aws-lambda": "^8.10.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "aws-lambda": "^1.0.0",
    "body-parser": "^2.2.2",
    "cors": "^2.8.5",
    "http-errors": "^2.0.0",
    "multer": "^2.0.1",
    "sharp": "^0.33.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "bestzip": "^2.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
